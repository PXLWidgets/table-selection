{
  "name": "@pxlwidgets/table-selection",
  "version": "0.9.2",
  "author": "Wouter Smit",
  "repository": "",
  "description": "Adds vertical and rectangular selection to HTML tables to allow copy of specific columns and rows without copying contents of other cells in range.",
  "keywords": [
    "table",
    "select",
    "selection",
    "column",
    "row",
    "cell"
  ],
  "license": "MIT",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./table-selection.d.ts",
  "scripts": {
    "dev": "run-p \"build:** -- --watch\"",
    "build": "run-p build:**",
    "build:cjs": "tsc",
    "build:esm": "tsc -m es6 --outDir esm",
    "build:umd": "webpack",
    "lint": "run-p lint:**",
    "lint:source": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "lint:declaration": "tslint -c tslint.json '*.d.ts' --fix",
    "prepublishOnly": "npm-run-all lint test:ci build",
    "test": "karma start karma.conf.js",
    "test:ci": "karma start --single-run --no-auto-watch karma.conf.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^7.0.0",
    "@types/webpack": "^4.4.19",
    "@types/webpack-env": "^1.13.6",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^3.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coveralls": "^2.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.5",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.2",
    "sinon": "^7.2.2",
    "ts-loader": "^5.3.1",
    "tslint": "^5.8.0",
    "typescript": "3.2.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
